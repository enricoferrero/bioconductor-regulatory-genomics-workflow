pages = {569--76},
pmid = {25915600},
publisher = {Nature Publishing Group},
title = {{Understanding multicellular function and disease with human tissue-specific networks.}},
url = {http://www.nature.com/doifinder/10.1038/ng.3259{\%}5Cnhttp://www.ncbi.nlm.nih.gov/pubmed/25915600{\%}5Cnhttp://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=PMC4828725},
