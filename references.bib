doi = {10.1128/JVI.00045-14},
file = {:C$\backslash$:/Users/ef884766/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Tang et al. - 2014 - Rab18 facilitates dengue virus infection by targeting fatty acid synthase to sites of viral replication(2).pdf:pdf},
