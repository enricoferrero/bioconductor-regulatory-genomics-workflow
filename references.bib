journal = {Epigenetics : official journal of the DNA Methylation Society},
keywords = {abbreviations,autoimmunity,encode,encyclopedia of dna elements,epigenetics,gene set enrichment analysis,genome,genome-wide association,genomerunner,gsea,gwas,ra,rheumatoid arthritis,sle,ssc,studies,systemic lupus erythematosus,systemic sclerosis or scleroderma,tfbs,transcription factor binding site},
