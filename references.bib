doi = {10.1038/ng.2231},
issn = {1061-4036},
journal = {Nature Genetics},
month = {mar},
number = {5},
pages = {511--516},
pmid = {22446963},
title = {{Meta-analysis identifies nine new loci associated with rheumatoid arthritis in the Japanese population}},
