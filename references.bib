issn = {1465-6914},
journal = {Genome biology},
month = {jan},
number = {5},
pages = {R74},
pmid = {24886562},
title = {{SoxNeuro orchestrates central nervous system specification and differentiation in Drosophila and is only partially redundant with Dichaete.}},
url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4072944{\&}tool=pmcentrez{\&}rendertype=abstract},
