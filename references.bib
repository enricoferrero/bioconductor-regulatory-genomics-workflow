journal = {Nature reviews. Genetics},
month = {mar},
number = {3},
pages = {129--45},
pmid = {26875678},
title = {{Unravelling the human genome-phenome relationship using phenome-wide association studies.}},
url = {http://www.nature.com/doifinder/10.1038/nrg.2015.36 http://www.ncbi.nlm.nih.gov/pubmed/26875678},
volume = {17},
