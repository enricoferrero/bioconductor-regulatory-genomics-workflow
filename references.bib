file = {:C$\backslash$:/Users/ef884766/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Levison et al. - 2013 - Genetic analysis of the Trichuris muris-induced model of colitis reveals QTL overlap and a novel gene cluster(2).pdf:pdf},
issn = {1471-2164},
journal = {BMC genomics},
keywords = {Animals,Chromosome Mapping,Chromosomes,Chromosomes: genetics,Colitis,Colitis: genetics,Colitis: parasitology,Genes,Genetic Predisposition to Disease,Genome-Wide Association Study,Genotype,Helminth,Inbred AKR,Inbred BALB C,Male,Mice,Microsatellite Repeats,Multigene Family,Phenotype,Polymorphism,Quantitative Trait Loci,Single Nucleotide,Trichuriasis,Trichuriasis: genetics,Trichuriasis: parasitology,Trichuris,Trichuris: genetics,Trichuris: pathogenicity},
